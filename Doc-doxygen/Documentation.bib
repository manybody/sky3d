@article{Mar05a,
Titel={Dipole giant resonances in deformed heavy nuclei},
author={J. A. Maruhn and P.--G. Reinhard and P. D. Stevenson  and 
   J. Rikovska Stone and M. R. Strayer},
journal={Phys. Rev. C},
volume={71},
year={2005},
pages={064328},
abstract={
The spectral distribution of isovector dipole strength is computed
using time-dependent Skyrme-Hartree-{F}ock with subsequent spectral
analysis. The calculations are done without any imposed symmetry restriction
allowing to deal with any nuclear shape. The scheme is used to study
the deformation dependence of the giant resonance and its interplay
with Landau fragmentation (due to $1ph$ states). Results are shown for
the chain of Nd isotopes, super-deformed $^{152}$Dy, triaxial
$^{188}$Os, and $^{238}$U.
}
} 
 

@article{Sim03a,
