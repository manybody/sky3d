@article{Ben00a,
title={Pairing Gaps from Nuclear Mean--Field Models},
author={M. Bender and K. Rutz and P.--G. Reinhard and  J. A. Maruhn},
journal={Eur. Phys. J.  A },
volume={8},
year={2000},
pages={59},
note={http://www.arxiv.org/abs/nucl-th/0005028},
abstract={
We discuss the pairing gap, the most important observable of nuclear
pairing correlations, in the framework of self--consistent nuclear mean--field
models. The equations for the conventional BCS model and the approximate
projection--before--variation Lipkin--Nogami method are formulated in
terms of local density functionals for the effective interaction.
We perform an approximate particle--number projection of both the
mean--field energy and the pairing energy.
Various definitions of the pairing gap are discussed as three--point,
four--point and five--point mass--difference formulas,
averaged matrix elements of the pairing potential
and single--quasiparticle energies.
Experimental values for the pairing gap are compared with calculations
employing both a delta pairing force and a density--dependent delta
interaction in the BCS and Lipkin--Nogami model. We find that the
five--point mass difference formula gives a very good approximation
for the pairing gap, other approximations may differ up to $30\%$
from the five--point gaps for certain nuclei.
\pacs{21.60.Jz, 21.30.Fe, 21.60.-n}     
}
}

@article{Loe11a,
  title =   {Equilibration in the time-dependent {H}artree-{F}ock approach
             probed with the {W}igner distribution function},
  author =  {N. Loebl and J. A. Maruhn and P.--G. Reinhard},
  journal = {Phys. Rev. C},
  volume =  {84},
  year =    {2011},
  pages =   {034608},
  doi={10.1103/PhysRevC.84.034608},
  abstract= {
By calculating the Wigner distribution function in the
reaction plane, we are able to probe the phase-space behavior in
time-dependent Hartree-Fock during a heavy-ion collision in a
consistent framework. \PGR{Various} 
expectation values of operators are calculated
by evaluating the corresponding integrals over the
Wigner function. In this approach it is straightforward to define and
analyze quantities even locally. We compare the Wigner distribution
function with the smoothed Husimi distribution function.  Different
reaction scenarios are presented by analyzing central and non-central
$^{16}O+$$^{16}O$ and $^{96}Zr+$$^{132}Sn$ collisions. Although we
observe strong dissipation in the time evolution of global
observables there is no evidence for complete equilibration in the
local analysis of the Wigner function. Due to the fact that the
initial
phase-space volumes of the fragments barely merge and mean values of
the observables are conserved in fusion reactions over thousands of
fm/c we conclude that time-dependent Hartree-Fock provides a good
description for the early stage of a heavy-ion collision but does not
provide a mechanism to change the phase space structure in a
dramatic way necessary to obtain complete equilibration. 
}
}

 
