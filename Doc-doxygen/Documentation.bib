@article{Klu09a,
title={Variations on a theme by {S}kyrme},
author={P. Kl\"upfel  and P.--G. Reinhard and T. J. B\"urvenich and J. A. Maruhn},
journal={Phys.Rev. C},
volume={79},
year={2009},
pages={034310},
note={http://www.arxiv.org/abs/0804.3385},
abstract={
We present a survey of the phenomenological adjustment of the
parameters of the Skyrme-Hartree-Fock (SHF) model for a
self-consistent description of nuclear structure and low-energy
excitations. A large sample of reliable input data from nuclear bulk
properties (energy, radii, surface thickness) is selected guided by
the criterion that ground-state correlations should remain small.
Least squares fitting techniques are used to determine the SHF
parameters which accommodate best the given input data. The question of
the predictive value of the adjustment is scrutinized by performing
systematic variations with respect to chosen nuclear matter properties
(incompressibility, effective mass, symmetry energy, and sum-rule
enhancement factor).  We find that the ground-state data, although
representing a large sample, leave a broad range of choices, i.e. a
broad range of nuclear matter properties. Information from giant
resonances is added to pin down more precisely the open features. We
then apply the set of newly adjusted parameterizations to several more
detailed observables such as neutron skin, isotope shifts, and super-heavy
elements. The techniques of least-squares fitting provide safe
estimates for the uncertainties of such extrapolations. The
systematic variation of forces allows to disentangle the various
influences on a given observable and to estimate the predictive value
of the SHF model. The results depend very much on the observable
under consideration.
}
}

 
@article{Rei07a,
title={Nuclear Giant Resonances and Linear Response},
author={P.--G. Reinhard and Lu Guo and J. A. Maruhn},
preprint={nucl-th/0703044},
journal={Eur. Phys. J. A},
volume={32},
year={2007},
pages={19},
note={http://www.arxiv.org/abs/nucl-th/0703044},
abstract={
We search for nonlinear effects in nuclear giant resonances (GRs), in
particular the isovector dipole and the isoscalar quadrupole modes. To
that end, we employ a spectral analysis of time-dependent Hartree-Fock
(TDHF) dynamics using Skyrme forces. Based on TDHF calculations over a
wide range of excitation amplitudes, we explore the collectivity and
degree of harmonic motion in these modes.  Both GR modes turn out to
be highly harmonic in heavy nuclei from A=100 on. There is no trace of
a transition to irregular motion and multiple resonances are
predicted. Slight anharmonicities are seen for light nuclei,
particularly for $^{16}$O. These are mainly caused by the spin-orbit
splitting.
}
}

 
@article{Ben03aR,
title={Self-Consistent Mean-Field Models for Nuclear Structure},
author={M. Bender and P.--H. Heenen and P.--G. Reinhard},
journal={Rev. Mod. Phys. },
volume={75},
year={2003},
pages={121},
abstract={
We review the present status of self-consistent mean-field models
(SCMF) for describing nuclear structure and low-energy dynamics. They
are presented as effective energy-density functionals. The three most
widely used variants of SCMF based on a {S}kyrme energy functional, a
Gogny force, and relativistic mean-field Lagrangian
are considered side-by-side.  The
crucial role of the treatment of pairing correlations in
each case is pointed out.
%
We discuss the relations to other nuclear structure models
and present several extensions beyond mean field which are currently
used.
%
The phenomenological adjustment of the model parameters is discussed
in detail.  The quality of the performances of the SCMF
model is demonstrated for a broad
range of typical applications.
}
}
 
